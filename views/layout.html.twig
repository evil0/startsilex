<!DOCTYPE html>
<html>
<head>
    {% block head %}
        <title>{% block title %}{% endblock %}</title>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <meta name="description" content="{% block meta_desc %}{% trans %}meta_description{%  endtrans %}{% endblock %}">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="google-site-verification" content="{% trans %}google_verification{% endtrans %}" />

        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="/web/css/main.css">

        {% block pagination %}{% endblock %}
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>


    {% block google %}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

            ga('create', '{% block google_analytics %}{% endblock %}', 'auto');
            ga('send', 'pageview');

        </script>
    {% endblock %}

    {% endblock %}


</head>
<body>
{% block header %}
    {%  include 'components/header.html.twig' %}
{% endblock %}

<div class="container">


    {% block body %}

    {% endblock %}

    <footer>
        <p>&copy; 2016 Company, Inc.</p>
    </footer>
</div> <!-- /container -->
</body>
</html>